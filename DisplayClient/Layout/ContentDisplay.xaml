<UserControl
    x:Class="DisplayClient.ContentDisplay"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:DisplayClient"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300"
    d:DesignWidth="400">

    <Grid Name="rootGrid">
        <Image Grid.Row="0" Name="DisplayingJobResult" Visibility="Collapsed"></Image>
        <Image Grid.Row="1" Name="DisplayingImage" Visibility="Collapsed"></Image>
        <WebView Name="DisplayingWebView" Visibility="Collapsed"></WebView>
        <MediaElement Name="DisplayingVideo" Visibility="Collapsed" AutoPlay="True" IsLooping="True" AreTransportControlsEnabled="True"></MediaElement>
    </Grid>
</UserControl>